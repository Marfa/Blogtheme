  <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js></script>
  {{! Ghost outputs important scripts and data with this tag }}
  {{ghost_foot}}

  {{!--<script type="text/javascript" src="{{asset "js/min/instafeed.min.js"}}"></script>--}}
  <script type="text/javascript" src="{{asset "js/min/jquery.fitvids.min.js"}}"></script>
  <script type="text/javascript" src="{{asset "js/min/jquery.ghostHunter.min.js"}}"></script>
  <script type="text/javascript" src="{{asset "js/min/pace.min.js"}}"></script>
  <script type="text/javascript" src="{{asset "js/min/simple-lightbox.min.js"}}"></script>
  <script type="text/javascript" src="{{asset "js/min/wow.min.js"}}"></script>
  {{! The main JavaScript file for Blacklist }}
  <script type="text/javascript" src="{{asset "js/min/index.min.js"}}"></script>

  {{#is "post"}}
    <script type="text/javascript">
      if ( typeof disqus_shortname !== 'undefined' ) {
        /* --- Don't edit below this line --- */
