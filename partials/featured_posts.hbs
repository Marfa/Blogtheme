<section class="featured-posts container wow fadeIn">
    {{#get "posts" filter="featured:true" limit="10" as |featured|}}
        {{#foreach featured}}
            <a href="{{url}}" class="featured-posts__post col-4">
                <div class="featured-posts__post-background">
                    {{# if feature_image}}
                    <img class="" src="{{img_url feature_image}}">
                    {{/if}}
                </div>
                <h1 class="featured-posts__post-title">ğŸ”¥ {{title}}</h1>
            </a>
        {{/foreach}}
    {{/get}}
</section>